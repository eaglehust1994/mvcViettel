	<parameter name="listStockId" class="java.util.List"/>
	<queryString>
		<![CDATA[SELECT OD.CODE code,
(CASE WHEN OD.TYPE=1 THEN 'Nhập' ELSE 'Xuất' END) type,
APP.NAME bussTypeName,
ST.NAME stockName,
ST.CODE stockCode,
OD.CREATED_BY_NAME createdByName,
(CASE WHEN OD.SIGN_STATE=1 THEN 'Chưa trình ký' WHEN OD.SIGN_STATE=2 THEN 'Đã trình ký' WHEN OD.SIGN_STATE=3 THEN 'Đã ký duyệt' ELSE 'Hủy ký' END) sidnState,
OD.CREATED_DEPTED_NAME createdDeptName,
OD.DESCRIPTION description
FROM WMS_OWNER_KTTS."ORDER" OD JOIN CAT_OWNER.STOCK ST ON ST.STOCK_ID = OD.STOCK_ID
LEFT JOIN CTCT_CAT_OWNER.APP_PARAM APP ON (APP.CODE=OD.BUSSINESS_TYPE AND APP.PAR_TYPE='IMPORT_ORDER_TYPE' AND OD.TYPE=1) OR (APP.CODE=OD.BUSSINESS_TYPE AND APP.PAR_TYPE='EXPORT_ORDER_TYPE' AND OD.TYPE=2)
WHERE OD.STATUS =1 AND ($X{IN,OD.STOCK_ID,listStockId})]]>
	</queryString>
