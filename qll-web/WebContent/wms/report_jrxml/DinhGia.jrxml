	<queryString>
		<![CDATA[SELECT WS.SHIPMENT_ID shipmentId,
                WS.TOTAL_ORIGIN_MONEY totaloriginMoney,
                WS.TOTAL_FEE totalFee,
                WS.TOTAL_TAX totalTax,
                WS.TOTAL_MONEY totalMoney,
                WS.DESCRIPTION description,
                ST.SHIPMENT_TAX_ID shipmentTaxId,
                ST.VALUE valueTax,
                CT.TAX_ID taxId,
                CT.NAME nameTax,
                CT.VALUE value1,
                (CASE WHEN CT.TYPE= '1' THEN 'Ðầu vào' ELSE 'Ðầu ra' END) typeTax,
                (CASE WHEN CT.IGNORE= '1' THEN 'Có' ELSE 'Không' END) ignoreTax,
                (CASE WHEN CT.APPLY= '1' THEN 'Có' ELSE 'Không' END) applyTax
                FROM WMS_OWNER_KTTS.SHIPMENT WS
                LEFT JOIN WMS_OWNER_KTTS.SHIPMENT_TAX ST ON WS.SHIPMENT_ID = ST.SHIPMENT_ID
                LEFT JOIN CAT_OWNER.TAX CT ON CT.TAX_ID = ST.TAX_ID WHERE 1=1 and WS.SHIPMENT_ID= $P{shipmentTaxId} ;]]>
	</queryString>
